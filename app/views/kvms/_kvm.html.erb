<div id="<%= dom_id kvm %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= kvm.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= kvm.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Unit of measure:</strong>
    <%= kvm.unit_of_measure %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Is active:</strong>
    <%= kvm.is_active? ? "Yes" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Organization:</strong>
    <%= kvm.organization_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Kva:</strong>
    <%= kvm.kva_id %>
  </div>
</div>
