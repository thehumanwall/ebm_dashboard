<div id="<%= dom_id measurement %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Value:</strong>
    <%= measurement.value %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Measurement date:</strong>
    <%= measurement.measurement_date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Notes:</strong>
    <%= measurement.notes %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Kvm:</strong>
    <%= measurement.kvm_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Experiment:</strong>
    <%= measurement.experiment_id %>
  </div>
</div>
